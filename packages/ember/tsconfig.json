{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "dummy/tests/*": [
        "tests/*"
      ],
      "dummy/*": [
        "tests/dummy/app/*"
      ],
      "@milestones/ember": [
        "addon"
      ],
      "@milestones/ember/*": [
        "addon/*"
      ],
      "@milestones/core": [
        "../../node_modules/@milestones/core/src"
      ],
      "@milestones/babel-plugin-strip-milestones": [
        "../../node_modules/@milestones/babel-plugin-strip-milestones/src"
      ],
      "*": [
        "types/*"
      ]
    }
  },
  "include": [
    "addon/**/*",
    "tests/**/*",
    "types/**/*"
  ]
}
